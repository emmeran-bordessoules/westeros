BEGIN;
CREATE TABLE "vote_scoredep" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "scoreDep" integer NOT NULL, "NumDep_id" integer NOT NULL REFERENCES "vote_departement" ("id"), "VoteDep_id" integer NOT NULL REFERENCES "vote_vote" ("NumVote"));
CREATE TABLE "vote_votant__new" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "IPVotant" varchar(100) NOT NULL, "NumVote_id" integer NOT NULL REFERENCES "vote_vote" ("NumVote"));
INSERT INTO "vote_votant__new" ("NumVote_id", "id", "IPVotant") SELECT "NumVote_id", "id", "IPVotant" FROM "vote_votant";
DROP TABLE "vote_votant";
ALTER TABLE "vote_votant__new" RENAME TO "vote_votant";
CREATE INDEX "vote_scoredep_df5c358a" ON "vote_scoredep" ("NumDep_id");
CREATE INDEX "vote_scoredep_4f0624d5" ON "vote_scoredep" ("VoteDep_id");
CREATE INDEX "vote_votant_ba238950" ON "vote_votant" ("NumVote_id");
CREATE TABLE "vote_votant__new" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "IPVotant" varchar(100) NOT NULL);
INSERT INTO "vote_votant__new" ("id", "IPVotant") SELECT "id", "IPVotant" FROM "vote_votant";
DROP TABLE "vote_votant";
ALTER TABLE "vote_votant__new" RENAME TO "vote_votant";

COMMIT;
