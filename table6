BEGIN;
CREATE TABLE "comment_comment__new" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "auteur" varchar(200) NOT NULL, "text" text NOT NULL);
INSERT INTO "comment_comment__new" ("id", "auteur", "text") SELECT "id", "author", "text" FROM "comment_comment";
DROP TABLE "comment_comment";
ALTER TABLE "comment_comment__new" RENAME TO "comment_comment";
CREATE TABLE "comment_comment__new" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "auteur" varchar(200) NOT NULL, "texte" text NOT NULL);
INSERT INTO "comment_comment__new" ("texte", "id", "auteur") SELECT "text", "id", "auteur" FROM "comment_comment";
DROP TABLE "comment_comment";
ALTER TABLE "comment_comment__new" RENAME TO "comment_comment";

COMMIT;
