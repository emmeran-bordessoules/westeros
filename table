BEGIN;
CREATE TABLE "vote_departement" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "NumDep" integer NOT NULL, "NomDep" varchar(200) NOT NULL);
CREATE TABLE "vote_votant" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "IPVotant" varchar(100) NOT NULL, "NumDep_id" integer NULL REFERENCES "vote_departement" ("id"));
CREATE TABLE "vote_vote" ("NumVote" integer NOT NULL PRIMARY KEY, "Score" integer NOT NULL, "NomVote" varchar(100) NOT NULL, "ImgVote" varchar(100) NOT NULL);
CREATE TABLE "vote_votant__new" ("id" integer NOT NULL PRIMARY KEY AUTOINCREMENT, "IPVotant" varchar(100) NOT NULL, "NumDep_id" integer NULL REFERENCES "vote_departement" ("id"), "NumVote_id" integer NOT NULL REFERENCES "vote_vote" ("NumVote"));
INSERT INTO "vote_votant__new" ("IPVotant", "id", "NumVote_id", "NumDep_id") SELECT "IPVotant", "id", NULL, "NumDep_id" FROM "vote_votant";
DROP TABLE "vote_votant";
ALTER TABLE "vote_votant__new" RENAME TO "vote_votant";
CREATE INDEX "vote_votant_df5c358a" ON "vote_votant" ("NumDep_id");
CREATE INDEX "vote_votant_ba238950" ON "vote_votant" ("NumVote_id");

COMMIT;
